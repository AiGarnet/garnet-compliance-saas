{
  "name": "garnet-ai-data-importer",
  "private": true,
  "version": "1.0.0",
  "description": "Data importer for Garnet AI compliance frameworks",
  "type": "module",
  "scripts": {
    "dev:next": "cd frontend && npm run dev",
    "dev:vite": "vite",
    "dev": "npm run dev:next",
    "prebuild": "node scripts/prebuild.js",
    "build:next": "cd frontend && npm run build",
    "build:vite": "tsc --skipLibCheck && vite build",
    "check-deps": "node scripts/check-deps.js",
    "build": "npm run prebuild && npm run build:next && npm run build:vite && node scripts/merge-builds.js",
    "preview": "vite preview",
    "dev:mock": "NODE_ENV=mock npm run seed:test && npm run dev",
    "seed:test": "node scripts/seed-test-data.js"
  },
  "dependencies": {
    "@faker-js/faker": "^9.8.0",
    "dotenv": "^16.5.0",
    "lucide-react": "^0.294.0",
    "moment": "^2.29.4",
    "pg": "^8.16.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.15.18",
    "@types/pg": "^8.15.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.4",
    "fs-extra": "^11.1.1",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.8.3",
    "vite": "^5.0.0"
  }
}
